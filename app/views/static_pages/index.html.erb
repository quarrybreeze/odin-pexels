<%= form_with url: static_pages_path, method: :get do |form| %>
  <%= form.label "Collection ID"%> <br>
  <%= form.text_field :collection_id %> <br>
  <%= form.submit "Search"%> 
<% end %>
<br>
<% if @photos.present? %>
  <% @photos.each do |photo| %>
    <img src="<%= photo.src['medium']%>"> <br>
    Credit: <%= photo.user.name%> from Pexel.<br><br>
  <% end %>
<% end %>